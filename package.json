{
  "type": "module",
  "name": "@nfdi4plants/arctrl",
  "version": "2.5.1",
  "description": "Top level ARC DataModel and API function descriptions.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfdi4plants/ARCtrl.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nfdi4plants/ARCtrl/issues"
  },
  "homepage": "https://github.com/nfdi4plants/ARCtrl#readme",
  "author": "Heinrich Lukas Weil <weil@rptu.de> (https://github.com/HLWeil)",
  "contributors": [
    "Kevin Frey <Freymaurer@gmx.de> (https://github.com/Freymaurer)"
  ],
  "main": "./dist/ts/index.umd.js",
  "module": "./dist/ts/index.es.js",
  "types": "./dist/ts/index.d.ts",
  "private": false,
  "files": [
    "dist/ts"
  ],
  "scripts": {
    "transpile": "dotnet fable clean -e fs.ts --yes && npm run fable -- --fableLib @fable-org/fable-library-js --noCache -e fs.ts -o src/ARCtrl/ts",
    "build": "vite build",
    "fable": "dotnet fable ./src/ARCtrl/ARCtrl.Javascript.fsproj --lang ts"
  },
  "exports": {
    ".": {
      "import": "./dist/ts/index.es.js",
      "require": "./dist/ts/index.umd.js",
      "types": "./dist/ts/index.d.ts"
    }
  },
  "dependencies": {
    "@fable-org/fable-library-js": "^1.8.0",
    "@nfdi4plants/exceljs": "^0.3.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.1",
    "jsonschema": "^1.4.1",
    "cypress": "^13.5.0",
    "mkdirp": "3.0.1",
    "mocha": "^10.2.0",
    "vite": "^4.5.11",
    "vite-plugin-dts": "^4.5.3",
    "vue": "^3.3.8"
  }
}
